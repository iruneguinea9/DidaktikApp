<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.Dina.Main" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/azulPrimario</item>
        <item name="colorPrimaryVariant">@color/azulSecundario</item>
        <item name="colorOnPrimary">@color/blanco</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/azulPrimario</item>
        <item name="colorSecondaryVariant">@color/azulSecundario</item>
        <item name="colorOnSecondary">@color/blanco</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:forceDarkAllowed">false</item>
    </style>
</resources>